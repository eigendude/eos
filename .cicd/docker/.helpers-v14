# Don't wrap cd or anything that wouldn't do well in a subshell
function execute() {
    ${TRAVIS:-false} && echo -en "travis_fold:start:$(echo $@)\r" || true
    echo "--- Executing: $@"
    "$@"
    echo $?
    [[ $? != 0 ]] && exit $?
    ${TRAVIS:-false} && echo -en "travis_fold:end:$(echo $@)\r" || echo "-- Execution Complete" # Cannot use ${TRAVIS:-false} here or it will allow failures to proceed
}