# Don't wrap cd or anything that wouldn't do well in a subshell
function execute() {
    ${TRAVIS:-false} && echo -en "travis_fold:start:$(echo $@)\r" || true
    echo "--- Executing: $@"
    "$@"
    [[ $TRAVIS == true ]] && echo -en "travis_fold:end:$(echo $@)\r" # Cannot use ${TRAVIS:-false} here or it will allow failures to proceed
}